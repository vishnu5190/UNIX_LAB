[?2004h]0;bmscecse@bmscecse-HP-Elite-Tower-600-G9-Desktop-PC: ~/LAB4[01;32mbmscecse@bmscecse-HP-Elite-Tower-600-G9-Desktop-PC[00m:[01;34m~/LAB4[00m$ cat swapNumbers
[?2004l#!/bin/sh/

#swap two numbers without a temporary variable

echo "enter num1: \c"
read num1

echo "enter num2: \c"
read num2

echo "before swapping: $num1 and $num2"

num1=$(expr $num1 + $num2)
num2=$(expr $num1 - $num2)
num1=$(expr $num1 - $num2)
