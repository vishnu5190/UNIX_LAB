#!/bin/sh/

# exit status of the command "cat /shadow/try"

# THIS SCRIPT IS WRONG

if [ `cat /shadow/try` $? -eq 0 ] 
then
echo "command succeeded"
echo "exit status: $?"

elif [ `cat /shadow/try` $? -ne 0 ]
then 
echo "command failed"
echo "exit status: $?"

else 
exit 1

fi
